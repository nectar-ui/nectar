{
  "name": "@nectar-ui/postcss-plugin",
  "version": "0.1.0",
  "description": "The nectarUi postcss plugin.",
  "author": "Brennan Walsh <mail@brennanwal.sh> (https://brennanwal.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nectar-ui/nectar.git",
    "directory": "packages/postcss-plugin"
  },
  "access": "public",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./reset": "./src/reset.css"
  },
  "publishConfig": {
    ".": {
      "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs.js"
      }
    },
    "./reset": "./dist/reset.css"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "vitest run",
    "build": "vite build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nectar-ui/tokens": "workspace:*",
    "csstype": "^3.1.2",
    "cosmiconfig": "^8.1.3",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "modern-normalize": "^1.1.0",
    "lodash.merge": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18",
    "postcss": "^8"
  },
  "build": {
    "name": "nectar",
    "entry": "src/index.ts",
    "es": "index.js",
    "cjs": "index.cjs.js"
  }
}
