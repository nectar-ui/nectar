{
	"name": "@nectar-ui/nectar",
	"version": "0.1.0",
	"description": "The nectarUi postcss plugin.",
	"author": "Brennan Walsh <mail@brennanwal.sh> (https://brennanwal.sh)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nectar-ui/nectar.git",
		"directory": "packages/nectar"
	},
	"access": "public",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./reset": "./src/reset.css"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup ./src/index.ts --config ../../tsup.config.ts --shims true"
	},
	"dependencies": {
		"@nectar-ui/components": "workspace:*",
		"@nectar-ui/tokens": "workspace:*",
		"csstype": "^3.1.2",
		"cosmiconfig": "^8.1.3",
		"cosmiconfig-typescript-loader": "^4.3.0",
		"modern-normalize": "^1.1.0",
		"lodash.merge": "^4.6.2"
	},
	"peerDependencies": {
		"react": "^16.8 || ^17 || ^18",
		"postcss": "^8",
		"@types/node": "*",
		"ts-node": ">=10",
		"typescript": ">=3"
	},
	"peerDependenciesMeta": {
		"@types/node": {
			"optional": true
		},
		"ts-node": {
			"optional": true
		},
		"typescript": {
			"optional": true
		}
	}
}
