{
	"configurations": [
		{
			"name": "node",
			"type": "node",
			"request": "launch",
			"runtimeArgs": ["--loader", "ts-node/esm"],
			"cwd": "${fileDirname}",
			"args": ["${file}"]
		},
		{
			"name": "Jest",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/node_modules/jest/bin/jest",
			"args": ["${fileBasename}"]
		}
	]
}
