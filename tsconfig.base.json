{
	"compilerOptions": {
		"composite": true,
		"customConditions": [],
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext"
	},
	"ts-node": {
		"esm": true
	}
}
